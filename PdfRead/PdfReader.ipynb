    "def text_cleaning(text):\n",
    "    cleaned_text = text.replace('\\n','')                         # remove \\n\n",
    "    cleaned_text = re.sub(r'[^\\w\\s]', ' ', cleaned_text)         # only letters\n",
    "    cleaned_text = re.sub(r'\\d+', r'', str(cleaned_text))        # remove digits\n",
    "    cleaned_text = re.sub(r'\\b\\w{1,3}\\b', '', cleaned_text)      # remove chars length less than 3\n",
    "    cleaned_text = cleaned_text.replace('\\t\\r \\xa0\\x0c', '')     # remove weirds chars\n",
    "    cleaned_text = cleaned_text.replace('\\x0c', '')              # remove weirds chars\n",
    "    cleaned_text = re.sub(r' +', r' ', (cleaned_text))           # remove dublicated spaces\n",
    "    cleaned_text = cleaned_text.strip()                          # strip leading/trailing spaces\n",
    "    return cleaned_text"
   ]
