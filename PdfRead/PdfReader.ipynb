    "def get_data(path, pages=None):\n",
    "    if not pages:\n",
    "        pagenums = set()\n",
    "    else:\n",
    "        pagenums = set(pages) # unique pages\n",
    "        \n",
    "    manager = PDFResourceManager()\n",
    "    output = StringIO()\n",
    "    laparams = LAParams()\n",
    "    # converter object associated with manager, output and layoutparam objects\n",
    "    converter = TextConverter(manager, output, laparams=laparams)\n",
    "    interpreter = PDFPageInterpreter(manager, converter)\n",
    "\n",
    "    data = {}\n",
    "    positions = []\n",
    "    urls = []\n",
    "\n",
    "    fp = open(path, 'rb')\n",
    "    for page in PDFPage.get_pages(fp, pagenums):\n",
    "        interpreter.process_page(page)\n",
    "        if page.annots:\n",
    "            for a in pdftypes.resolve1(page.annots):\n",
    "                if isinstance(a, pdftypes.PDFObjRef):\n",
    "                    annotationDict = a.resolve()\n",
    "                    if 'A' in annotationDict:\n",
    "                        position = annotationDict['Rect']\n",
    "                        url = str(annotationDict['A']['URI'])\n",
    "                        url = url.lstrip('b') # is that always the same?\n",
    "                        positions.append(position)\n",
    "                        urls.append(url)\n",
    "                        # what about the annotation text?\n",
    "                    else:\n",
    "                        continue\n",
    "                else:\n",
    "                    sys.stderr.write('Warning: unknown annotation: %s\\n' % a)\n",
    "                    \n",
    "    fp.close()\n",
    "    converter.close()\n",
    "    text = output.getvalue()\n",
    "    output.close()\n",
    "    \n",
    "    data['filename'] = path.split('/')[-1]\n",
    "    data['raw_text'] = text\n",
    "    data['annotation_positions'] = positions\n",
    "    data['annotation_urls'] = urls\n",
    "\n",
    "    return data"
   ]
